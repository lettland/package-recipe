    "require": {
        "php": ">=8.2.0",
        "ext-json": "*",
        "composer-plugin-api": "*",
        "colinodell/indentation": "*",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/filesystem": "*",
        "symfony/finder": "*",
        "symfony/options-resolver": "*",
        "symfony/validator": "*",
        "symfony/var-exporter": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "composer/composer": "*"
    },
    "autoload": {
        "psr-4": {
            "Lettland\\PackageRecipe\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "extra": {
        "class": "Lettland\\PackageRecipe\\PackageRecipe",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "thanks": {
